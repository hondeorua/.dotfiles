return {
  'nanozuki/tabby.nvim',
  dependencies = { 'nvim-tree/nvim-web-devicons', lazy = true },
  config = function()
    local theme = {
      fill = 'TabLineFill',
      head = { fg = '#8aadf4', bg = 'none', style = 'italic' },
      current_tab = { fg = 'none', bg = '#cf174b', style = 'italic' },
      tab = { fg = 'none', bg = '#91505d', style = 'italic' },
      win = { fg = '#1e2030', bg = '#8aadf4', style = 'italic' },
      tail = { fg = '#8aadf4', bg = 'none', style = 'italic' },
    }

    require('tabby.tabline').set(function(line)
      return {
        {
          { ' üöÄ ', hl = theme.head },
          line.sep('', theme.head, theme.fill), -- ÓÇº
        },

        line.tabs().foreach(function(tab)
          local hl = tab.is_current() and theme.current_tab or theme.tab
          return {
            line.sep('ÓÇ∂', hl, theme.fill), -- ÓÇ∫
            tab.is_current() and 'ÔÜí' or 'ÔÑå',
            tab.number() .. '.',
            tab.name(),
            -- tab.current_win().file_icon(),
            -- tab.close_btn('ÔÄç'), -- show a close button
            line.sep('ÓÇ¥', hl, theme.fill), -- ÓÇº
            hl = hl,
            margin = ' ',
          }
        end),

        line.spacer(),
        -- shows list of windows in tab
        --line.wins_in_tab(line.api.get_current_tab()).foreach(function(win)
        --  return {
        --    line.sep('ÓÇ∫', theme.win, theme.fill),
        --    win.is_current() and 'ÔÜí' or 'ÔÑå',
        --    win.buf_name(),
        --    line.sep('ÓÇº', theme.win, theme.fill),
        --    hl = theme.win,
        --    margin = ' ',
        --  }
        --end),
        -- {
        --   line.sep('ÓÇ∫', theme.tail, theme.fill), -- ÓÇ∫
        --   { ' ÔÉ∂ ', hl = theme.tail },
        -- },
        hl = theme.fill,
      }
    end)
  end,
}
